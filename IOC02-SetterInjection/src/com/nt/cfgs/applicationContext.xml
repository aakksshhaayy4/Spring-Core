<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

<!-- Setter Injection for Student class -->
<bean id = "student" class = "com.nt.beans.Student">
<property name="sno" value="032"/>
<property name="studentName" value="Pavan"/>
<property name="address" value="Maharashtra"/>
<property name="marks" value="789"></property>
</bean>

<!-- Setter Injection for employee class -->
<bean id = "employee" class = "com.nt.beans.Employee">
<property name="empId" value="01"/>
<property name="empName" value="Akshay"/>
<property name="address" value="Maharashtra"/>
<property name="salary" value="25800"/>
</bean>

</beans>
